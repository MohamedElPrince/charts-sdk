{
  "icon": "./assets/icon.svg",
  "name": "Boxplot-session-1",
  "hint": "Needs a dimension and a measure, it will show the min, Q1, median, Q3 & max values of the measure",
  "options": {
    "aggregation": true,
    "individualFilter": true
  },
  "settings": [
    {
      "name": {
        "key": "Setting1"
      },
      "settings": [
        {
          "name": {
            "key": "toggle"
          },
          "key": "key1",
          "type": "text",
          "default": "Default Value"
        }
      ]
    }
  ],
  "bindingsTrays": [
    {
      "key": "tray-key",
      "name": "Dimension",
      "queryRole": "row",
      "minCount": 1,
      "maxCount": 1,
      "settings": [
        {
          "name": "Settings",
          "settings": [
            {
              "name": "Sorting",
              "key": "sort1",
              "type": "sort"
            }
          ]
        }
      ]
    },
    {
      "key": "tray-key2",
      "name": "Measure",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1,
      "settings": [
        {
          "name": "Number Format",
          "settings": [
            {
              "name": "Scale",
              "key": "scale1",
              "type": "scale"
            },
            {
              "name": "Format",
              "key": "format",
              "type": "number-format",
              "defaultValue": "###,###"
            },
            {
              "name": "Conditional Formatting",
              "key": "cond-format2",
              "type": "conditional-formatting"
            },
            {
              "name": "Running Total",
              "key": "running-total",
              "type": "running-total"
            }
          ]
        },
        {
          "name": "Filters",
          "settings": [
            {
              "name": "Filter",
              "key": "filter2",
              "type": "filter"
            }
          ]
        }
      ]
    }
  ]
}
